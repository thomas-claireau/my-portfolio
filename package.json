        "issues"
      ]
    }
  },
  "dependencies": {
    "next": "12.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-parse": "^10.0.1",
    "remark-react": "^9.0.1",
    "sass": "^1.52.2",
    "simple-oauth2": "^4.3.0",
    "tailwindcss": "^3.0.23",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^17.0.43",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.2",
    "eslint-plugin-import": "^2.25.4",
